parameters:
    level: 5
    paths:
        - src
    excludePaths:
        - vendor/
        - node_modules/
    reportUnmatchedIgnoredErrors: false
    ignoreErrors:
        - '#Function .+ not found#'
        - '#Parameter .+ has invalid type#'
        - '#Right side of && is always true#'
    bootstrapFiles:
        - vendor/php-stubs/wordpress-stubs/wordpress-stubs.php
        - vendor/szepeviktor/phpstan-wordpress/bootstrap.php
