    "lint:css": "stylelint src/css",
    "lint": "npm run lint:js && npm run lint:css",
    "format": "prettier --write \"src/**/*.{js,css,scss}\""
  },
  "devDependencies": {
    "vite": "^5.4.0",
    "autoprefixer": "^10.4.20",
    "cssnano": "^7.0.6",
    "postcss": "^8.4.47",
    "terser": "^5.34.1"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "not dead"
  ],
  "keywords": [
    "wordpress",
    "theme",
    "atk-ved",
    "logistics",
    "china",
    "marketplace"
