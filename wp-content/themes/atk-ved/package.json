{
  "name": "atk-ved-theme",
  "version": "3.3.0",
  "description": "АТК ВЭД — современная тема WordPress для логистической компании",
  "private": true,
  "author": "ATK VED Team",
  "license": "GPL-2.0-or-later",
  "homepage": "https://atk-ved.ru",
  "repository": {
    "type": "git",
    "url": "https://github.com/atk-ved/theme.git"
  },
  "bugs": {
    "url": "https://github.com/atk-ved/theme/issues"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "watch": "vite build --watch",
